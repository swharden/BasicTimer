<Window x:Class="BasicTimer.SetTitleWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BasicTimer"
        mc:Ignorable="d"
        Title="Set Title" Height="150" Width="300"
        WindowStartupLocation="CenterScreen"
        >
    <StackPanel Margin="20 0" VerticalAlignment="Center">
        <TextBox x:Name="TitleTextbox" Margin="0 0 0 10" FontSize="16" KeyDown="TitleTextbox_KeyDown">New Title</TextBox>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
            <Button FontSize="16"  Padding="5 0" Margin="0 0 10 0" Click="Button_OK_Click">OK</Button>
            <Button FontSize="16" Padding="5 0" Click="Button_Cancel_Click">Cancel</Button>
        </StackPanel>
    </StackPanel>
</Window>
